{
    "name": "ivene/autocode",
    "type": "library",
    "description": "Laravel Tools",

    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yao.Yao",
            "email": "yao_ivene@163.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "laravelcollective/html": "^6.3",
        "symfony/var-exporter": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.5.1"
    },
    "autoload": {
        "psr-4": {
            "Ivene\\AutoCode\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Ivene\\AutoCode\\IveneAutoCodeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
